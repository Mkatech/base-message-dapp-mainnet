# Copy this file to `.env` and fill values. Do NOT commit .env to GitHub.

# Deployer private key (wallet that has ETH on Base)
PRIVATE_KEY=0xyourprivatekeyhere

# Optional custom RPC URLs (defaults are fine if you leave blank)
BASE_SEPOLIA_RPC=https://sepolia.base.org
BASE_MAINNET_RPC=https://mainnet.base.org

# Optional: if you want to verify contracts on Basescan
BASESCAN_API_KEY=your_basescan_api_key_here
